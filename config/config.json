{
  "$schema": "https://dev.office.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "spfx-template-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/spfxTemplate/SpfxTemplateWebPart.js",
          "manifest": "./src/webparts/spfxTemplate/SpfxTemplateWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {
    "jquery": {
      "path": "node_modules/jquery/dist/jquery.min.js",
      "globalName": "jQuery"
    },
    "bootstrap": {
      "path": "node_modules/bootstrap/dist/js/bootstrap.min.js",
      "globalName": "bootstrap"
    }
  },
  "localizedResources": {
    "SpfxTemplateWebPartStrings": "lib/webparts/spfxTemplate/loc/{locale}.js"
  }
}
